{
  "name": "midlo",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "pnpm -r --workspace-concurrency=1 run build",
    "typecheck": "pnpm -r --workspace-concurrency=1 run typecheck",
    "dev:app": "pnpm --filter midlo-app start",
    "dev:web": "pnpm --filter midlo-web dev",
    "dev:desktop": "pnpm --filter midlo-desktop dev"
  }
}
